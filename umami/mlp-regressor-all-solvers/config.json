{
    "main-file"       : "run_task.py",
    "name"            : "mlp-regressor-all-solvers",
    "likelihood"      : "GAUSSIAN",
    "variables" : {
        "alpha" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0.00001,
            "max"  : 0.009
        },
        "hidden_layer_sizes" : {
            "type" : "INT",
            "size" : 3,
            "min"  : 1,
            "max"  : 200
        },
        "solver": {
            "type": "ENUM",
            "size": 1,
            "options": ["lbfgs", "sgd", "adam"]
        },
        "activation": {
            "type": "ENUM",
            "size": 1,
            "options": ["identity", "logistic", "tanh", "relu"]
        }
    }
}
